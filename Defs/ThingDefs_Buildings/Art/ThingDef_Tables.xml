	<ThingDef ParentName="BuildingBase">
		<defName>O21_ST_Table3x3Animated</defName>
		<label>federation infotable</label>
		<graphicData>
			<texPath>Things/Building/Federation/Decor/LCARSDesk/LCARSDesk_Inactive</texPath>
			<graphicClass>Graphic_Single</graphicClass>
			<shaderType>CutoutComplex</shaderType>
			<drawSize>(3, 3)</drawSize>
		</graphicData>
		<uiIconScale>0.9</uiIconScale>
		<statBases>
			<MaxHitPoints>175</MaxHitPoints>
			<WorkToBuild>3300</WorkToBuild>
			<Mass>30</Mass>
			<Flammability>1.0</Flammability>
			<Beauty>30</Beauty>
		</statBases>
        <comps>
            <li Class="O21Toolbox.Animation.CompProperties_AnimationOverlay">
                <damageThreshold>0.0</damageThreshold>
                <idleWithFuel>false</idleWithFuel>
                <settings>
                    <li>
                        <type>idle</type>
                        <graphicPath>Things/Building/Federation/Decor/LCARSDesk/LCARSDesk</graphicPath>
                        <frameCount>11</frameCount>
                        <fps>11</fps>
                    </li>
                </settings>
            </li>
			<li Class="CompProperties_Power">
				<compClass>CompPowerTrader</compClass>
        		<shortCircuitInRain>true</shortCircuitInRain>
				<basePowerConsumption>100</basePowerConsumption>
			</li>
			<li Class="CompProperties_Flickable"/>
			<li Class="CompProperties_Art">
				<nameMaker>NamerArtFurniture</nameMaker>
				<descriptionMaker>ArtDescription_Furniture</descriptionMaker>
				<minQualityForArtistic>Excellent</minQualityForArtistic>
			</li>
			<li>
				<compClass>CompQuality</compClass>
			</li>
        </comps>
		<size>(3,3)</size>
		<tickerType>Normal</tickerType>
		<costList>
			<Steel>80</Steel>
            <ComponentIndustrial>2</ComponentIndustrial>
		</costList>
		<rotatable>false</rotatable>
		<building>
			<buildingSizeCategory>Medium</buildingSizeCategory>
		</building>
		<designationCategory>O21_ST_StarTrek</designationCategory>
	</ThingDef>

